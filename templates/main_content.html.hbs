<section class="card">
  <h3>üõ†Ô∏è ~$ ls projects</h3>
  {{#if projects}}
  <div id="projectList">
    {{#each projects as |project index|}}
    <div class="project-item {{#if (gte index 2)}}hidden{{/if}}" data-project="{{index}}">
      <h4>{{project.name}}</h4>
      <p>{{project.description}}</p>
      <p>Technologies: {{#each project.tech as |tech|}}{{tech}}{{#unless @last}}, {{/unless}}{{/each}}</p>
      <a href="/redirect/{{project.repo}}" class="git-button"><i class="devicon-git-plain"></i>View on GIT</a>
    </div>
    {{#if @last}}
      {{#if (gte @index 2)}}
        <button id="showMoreProjects" class="git-button">Show More</button>
      {{/if}}
    {{/if}}
    {{/each}}
  </div>
  {{else}}
    <p>More Projects Coming Soon!</p>
  {{/if}}
</section>

