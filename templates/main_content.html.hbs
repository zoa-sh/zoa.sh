<section class="card mb-6 border-2 border-purple-500">
  <h3 class="text-2xl font-semibold mb-4">üõ†Ô∏è ~$ ls projects</h3>
  {{#if projects}}
  <div id="projectList">
    {{#each projects as |project index|}}
    <div class="project-item mb-4 {{#if (gte index 2)}}hidden{{/if}}" data-project="{{index}}">
      <h4 class="text-xl font-semibold">{{project.name}}</h4>
      <p>{{project.description}}</p>
      <p>Technologies: {{#each project.tech as |tech|}}{{tech}}{{#unless @last}}, {{/unless}}{{/each}}</p>
      <a href="/redirect/{{project.repo}}" class="git-button"><i class="devicon-git-plain"></i>View on GIT</a>
    </div>
    {{#if @last}}
      {{#if (gte @index 2)}}
        <button id="showMoreProjects" class="git-button mt-4">Show More</button>
      {{/if}}
    {{/if}}
    {{/each}}
  </div>
  {{else}}
    <p>More Projects Coming Soon!</p>
  {{/if}}
</section>

